{
    "name": "bitrix24/starter-kit-php-backend",
    "version": "0.0.1",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "darsyn/ip": "^5",
        "darsyn/ip-doctrine": "^5.0",
        "doctrine/dbal": "^3.10.0",
        "doctrine/doctrine-bundle": "^2.19.0",
        "doctrine/doctrine-migrations-bundle": "^3.5.0",
        "doctrine/orm": "^3.6.0",
        "firebase/php-jwt": "dev-main",
        "giggsey/libphonenumber-for-php": "^8.13.55",
        "mesilov/bitrix24-php-lib": "dev-dev",
        "nelmio/cors-bundle": "^2.0@dev",
        "nesbot/carbon": "^3",
        "odolbeau/phone-number-bundle": "^4.1",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.3.*",
        "symfony/monolog-bundle": "^3",
        "symfony/runtime": "7.3.*",
        "symfony/yaml": "7.3.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "^1",
        "phpstan/phpstan": "^1.12",
        "rector/rector": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*",
            "docker": true
        }
    }
}
